<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #faedcd;" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.database.dbadmin.controllers.MainPageController">
   <children>
      <MenuBar layoutY="1.0" prefHeight="22.0" prefWidth="600.0" style="-fx-background-color: #d4a373;">
        <menus>
          <Menu mnemonicParsing="false" text="Поставки">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#AddProvider" text="Добавить поставку" />
                <MenuItem mnemonicParsing="false" onAction="#AddSupply" text="Добавить поставщика" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Заказ">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#AddOrder" text="Добавить заказ" />
                <MenuItem mnemonicParsing="false" onAction="#AddClient" text="Добавить заказчика" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Книги">
            <items>
                <MenuItem mnemonicParsing="false" onAction="#AddBook" text="Добавить книгу" />
                <MenuItem mnemonicParsing="false" onAction="#library" text="Посмотреть библиотеку" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button layoutX="573.0" layoutY="1.0" mnemonicParsing="false" onAction="#close" prefHeight="18.0" prefWidth="26.0" style="-fx-background-color: #d4a373;" text="Х" />
   </children>
</AnchorPane>
